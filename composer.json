{
    "name": "awcodes/filament-gravatar",
    "type": "package",
    "description": "Replace Filament's default avatar url provider with one for Gravatar.",
    "keywords": [
        "framework",
        "laravel",
        "filament",
        "gravatar"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Adam Weston",
            "email": "awcodes1@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.0",
        "spatie/laravel-package-tools": "^1.15"
    },
    "require-dev": {
        "laravel/pint": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Awcodes\\FilamentGravatar\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Awcodes\\FilamentGravatar\\GravatarServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}