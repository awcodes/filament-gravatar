{
    "name": "awcodes/filament-gravatar",
    "type": "package",
    "description": "Replace Filament's default avatar url provider with one for Gravatar.",
    "keywords": [
        "framework",
        "laravel",
        "filament",
        "gravatar"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Adam Weston",
            "email": "awcodes1@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "filament/filament": "^2.0",
        "spatie/laravel-package-tools": "^1.9.2"
    },
    "autoload": {
        "psr-4": {
            "FilamentGravatar\\": "src/",
            "FilamentGravatar\\Database\\Factories\\": "database/factories/",
            "FilamentGravatar\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FilamentGravatar\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "FilamentGravatar\\FilamentGravatarServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}